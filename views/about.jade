extends layout

block navigation
	.navbar(class='navbar-inverse navbar-fixed-top')
		.navbar-inner
			.container
				button(type='button', class='btn btn-navbar', data-toggle='collapse', data-target='.nav-collapse')
					span.icon-bar
					span.icon-bar
					span.icon-bar
				a.brand(href='/')=title
				.nav-collapse(class='collapse')
					ul.nav
						li
							a(href='/') Home
						li.active
							a(href='/about') About
						li
							a(href='/contact') Contact
					.navbar-search(class='pull-right', action='')
						input(type='text', class='search-query span2', placeholder='Search')

block header
	.jumbotron(class='masthead')
		.container
			h2 About

block content
      
block footer
	footer.footer
		.container
			p.pull-right
				a(href='#') Back to top
      
      
      
      
      
      
      
      
      
      
      
      
      
      
